{
  "name": "SWIFT",
  "version": "1.5.0",
  "manifest_version": 3,
  "description": "An extension to keep you safe online.",
  "action": {
    "default_icon": "icons/icon-128.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "ext/background.js"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "storage",
    "identity"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "host_permissions": [
  ]
}
